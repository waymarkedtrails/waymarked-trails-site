<Style name="Shields">
  <Rule>
    <ShieldSymbolizer base="syms" file="[fname].svg" placement="point" spacing="400" face-name="DejaVu Sans Mono Bold" label-position-tolerance="200" minimum-distance="30" horizontal-alignment="middle" vertical-alignment="middle" minimum-path-length="10" avoid-edges="true" />
  </Rule>
</Style>

<Layer maximum-scale-denominator="25000" name="shields_ways" buffer-size="512">
  <Datasource base="psqlch">
      <Parameter name="table">(SELECT geom, symbol as fname FROM &dbschema;.slopeways WHERE char_length(symbol) > 7) AS w</Parameter>
    </Datasource>
  <StyleName>Shields</StyleName>
</Layer>

